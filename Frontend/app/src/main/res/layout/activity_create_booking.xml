<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#000000"
        tools:context=".Acitvity.CreateListingActivity">

        <ScrollView
            android:layout_width="400dp"
            android:layout_height="570dp"
            android:layout_marginTop="38dp"
            android:layout_marginEnd="5dp"
            android:layout_marginBottom="102dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp">

                <LinearLayout
                    android:layout_width="397dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    tools:layout_editor_absoluteX="8dp"
                    tools:layout_editor_absoluteY="106dp">

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:weightSum="2">


                    </TableRow>

                    <TableLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp">

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:id="@+id/fromDate"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:text="       From"
                                android:textColor="@color/white"
                                android:textSize="10sp" />

                            <EditText
                                android:id="@+id/editTextListingStartDate"
                                android:layout_width="67dp"
                                android:layout_height="46dp"
                                android:layout_margin="15dp"
                                android:background="@drawable/rounded_edit_text"
                                android:hint="@string/starting_date"
                                android:inputType="date"
                                android:textColor="@color/white"
                                android:textColorHint="#808080"
                                android:textSize="13sp" />

                            <TextView
                                android:id="@+id/fromTime"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:text="@string/from_time"
                                android:textColor="@color/white"
                                android:textSize="10sp" />

                            <EditText
                                android:id="@+id/editTextListingStartTime"
                                android:layout_width="86dp"
                                android:layout_height="41dp"
                                android:layout_margin="15dp"
                                android:background="@drawable/rounded_edit_text"
                                android:hint="@string/starting_time"
                                android:inputType="time"
                                android:textColor="@color/white"
                                android:textColorHint="#808080"
                                android:textSize="13sp" />


                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:id="@+id/toDate"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:text="         To"
                                android:textColor="@color/white"
                                android:textSize="10sp" />

                            <EditText
                                android:id="@+id/editTextListingEndDate"
                                android:layout_width="match_parent"
                                android:layout_height="36dp"
                                android:layout_margin="15dp"
                                android:background="@drawable/rounded_edit_text"
                                android:hint="@string/ending_date"
                                android:inputType="date"
                                android:textColor="@color/white"
                                android:textColorHint="#808080"
                                android:textSize="13sp" />

                            <TextView
                                android:id="@+id/toTime"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:text="@string/to_time"
                                android:textColor="@color/white"
                                android:textSize="10sp" />

                            <EditText
                                android:id="@+id/editTextListingEndTime"
                                android:layout_width="79dp"
                                android:layout_height="45dp"
                                android:layout_margin="15dp"
                                android:background="@drawable/rounded_edit_text"
                                android:hint="@string/ending_time"
                                android:inputType="time"
                                android:textColor="@color/white"
                                android:textColorHint="#808080"
                                android:textSize="13sp" />
                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:id="@+id/location_enter"
                                android:layout_width="50dp"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:text="     Location"
                                android:textColor="@color/white"
                                android:textSize="10sp" />

                            <EditText
                                android:id="@+id/editTextLocation"
                                android:layout_width="100dp"
                                android:layout_height="50dp"
                                android:layout_margin="15dp"
                                android:background="@drawable/rounded_edit_text"
                                android:hint="Enter Location"
                                android:inputType="text"
                                android:textColor="@color/white"
                                android:textColorHint="#808080"
                                android:textSize="13sp" />

                            <TextView
                                android:id="@+id/zip_enter"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="15dp"
                                android:text="Zipcode"
                                android:textColor="@color/white"
                                android:textSize="10sp" />

                            <EditText
                                android:id="@+id/editTextZipcode"
                                android:layout_width="84dp"
                                android:layout_height="49dp"
                                android:layout_margin="15dp"
                                android:background="@drawable/rounded_edit_text"
                                android:hint="Enter zip"
                                android:inputType="time"
                                android:textColor="@color/white"
                                android:textColorHint="#808080"
                                android:textSize="13sp" />


                        </TableRow>

                        <TableRow
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">


                        </TableRow>
                    </TableLayout>


                    <Button
                        android:id="@+id/viewAllBookings"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="15dp"
                        android:background="@android:color/transparent"
                        android:text="View All Available Properties"
                        android:textColor="@color/white" />


                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <CheckBox
                            android:id="@+id/checkbox_driveway"
                            android:layout_width="126dp"
                            android:layout_height="20dp"
                            android:layout_marginTop="12dp"
                            android:text="Driveway"
                            android:textColor="#FFFFFF"
                            android:textSize="10sp" />

                        <CheckBox
                            android:id="@+id/checkbox_car"
                            android:layout_width="113dp"
                            android:layout_height="20dp"
                            android:layout_marginTop="12dp"
                            android:text="Car"
                            android:textColor="#FFFFFF"
                            android:textSize="10sp" />

                        <CheckBox
                            android:id="@+id/checkbox_ev"
                            android:layout_width="109dp"
                            android:layout_height="20dp"
                            android:layout_marginTop="12dp"
                            android:text="EV Charging"
                            android:textColor="#FFFFFF"
                            android:textSize="10sp" />

                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <CheckBox
                            android:id="@+id/checkbox_garage"
                            android:layout_width="126dp"
                            android:layout_height="20dp"
                            android:layout_marginTop="12dp"
                            android:text="Garage"
                            android:textColor="#FFFFFF"
                            android:textSize="10sp" />

                        <CheckBox
                            android:id="@+id/checkbox_truck"
                            android:layout_width="113dp"
                            android:layout_height="20dp"
                            android:layout_marginTop="12dp"
                            android:text="Truck"
                            android:textColor="#FFFFFF"
                            android:textSize="10sp" />

                        <CheckBox
                            android:id="@+id/checkbox_inout"
                            android:layout_width="100dp"
                            android:layout_height="20dp"
                            android:layout_marginTop="12dp"
                            android:text="In Out"
                            android:textColor="#FFFFFF"
                            android:textSize="10sp" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <CheckBox
                            android:id="@+id/checkbox_parkinglot"
                            android:layout_width="126dp"
                            android:layout_height="20dp"
                            android:layout_marginTop="12dp"
                            android:text="Parking Lot"
                            android:textColor="#FFFFFF"
                            android:textSize="10sp" />

                        <CheckBox
                            android:id="@+id/checkbox_motorcycle"
                            android:layout_width="113dp"
                            android:layout_height="20dp"
                            android:layout_marginTop="12dp"
                            android:text="Motorcycle"
                            android:textColor="#FFFFFF"
                            android:textSize="10sp" />

                        <CheckBox
                            android:id="@+id/checkbox_tailgating"
                            android:layout_width="100dp"
                            android:layout_height="20dp"
                            android:layout_marginTop="12dp"
                            android:text="Tailgating"
                            android:textColor="#FFFFFF"
                            android:textSize="10sp" />
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <CheckBox
                            android:id="@+id/checkbox_handicapped"
                            android:layout_width="126dp"
                            android:layout_height="20dp"
                            android:layout_marginTop="12dp"
                            android:text="Handicapped"
                            android:textColor="#FFFFFF"
                            android:textSize="10sp" />

                        <CheckBox
                            android:id="@+id/checkbox_oversized"
                            android:layout_width="114dp"
                            android:layout_height="20dp"
                            android:layout_marginTop="12dp"
                            android:text="Oversized"
                            android:textColor="#FFFFFF"
                            android:textSize="10sp" />

                        <CheckBox
                            android:id="@+id/checkbox_shuttle"
                            android:layout_width="100dp"
                            android:layout_height="20dp"
                            android:layout_marginTop="12dp"
                            android:text="Shuttle"
                            android:textColor="#FFFFFF"
                            android:textSize="10sp" />
                    </TableRow>


                </LinearLayout>
            </FrameLayout>
        </ScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
